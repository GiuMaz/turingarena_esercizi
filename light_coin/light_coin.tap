interface light_coin_interfaces {

    var int N;

    function find_light_coin() -> int;

    callback place(int coin, int position) {
        output coin, position;
    }

    callback weigh() -> int {
        flush;
        var int ans;
        input ans;
        return ans;
    }

    main {
        input N;
        var int ans;
        call find_light_coin() -> ans;
        output ans;
    }
}

